<?xml version="1.0" encoding="utf-8"?>

<!--
	
-->
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009"
				   xmlns:s="library://ns.adobe.com/flex/spark"
				   xmlns:fb="http://ns.adobe.com/flashbuilder/2009"
				   alpha.disabled="0.5"
				   minWidth="110" minHeight="110">
	<fx:Metadata>
		<![CDATA[ 
		/** 
		* @copy spark.skins.spark.ApplicationSkin#hostComponent
		*/
		[HostComponent("spark.components.Button")]
		]]>
	</fx:Metadata>
	<fx:Script>
		<![CDATA[
			import com.tang.common.Icons;
		]]>
	</fx:Script>
	
    <!-- states -->
    <s:states>
        <s:State name="up" />
        <s:State name="over" />
        <s:State name="down" />
        <s:State name="disabled" />
    </s:states>
    
	<s:Graphic left="0" top="0" right="0" bottom="0" horizontalCenter="0" alpha.over="0.8">
		<s:BitmapImage id="iconDisplay" source="{Icons.ICON_WEB}" />
	</s:Graphic>
	
    <!-- layer 8: text -->
    <!--- @copy spark.components.supportClasses.ButtonBase#labelDisplay  -->
    <s:Label id="labelDisplay" left="5" right="5" bottom="10" maxDisplayedLines="1"
			 textAlign="center" verticalAlign="middle"
			 color="#ffffff">
    </s:Label>
    
</s:SparkSkin>
